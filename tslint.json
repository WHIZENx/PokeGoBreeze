{
  "defaultSeverity": "warning",
  "extends": [
    "tslint:recommended",
    "tslint-react",
    "tslint-config-prettier"
  ],
  "rulesDirectory": [
    "tslint-plugin-prettier"
  ],
  "rules": {
    "prettier": true,
    "interface-name": false,
    "jsx-no-lambda": false,
    "jsx-no-bind": false,
    "radix": false,
    "no-shadowed-variable": false,
    "one-variable-per-declaration": false
  }
}